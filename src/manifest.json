{
    "name": "HYKW Backlog Quick button",
    "version": "1.1",
    "manifest_version": 2,
    
    "description": "Backlog を便利に使う",

    "permissions": [
        "tabs", "https://*.backlog.jp/*"
    ],

    "background": {
        "scripts": ["background.js"]
    },
    
    "page_action": {
        "default_icon": "icon.png",
        "default_title": "HYKW Backlog Quick button",
        "default_popup": "popup.html"
    }
}

